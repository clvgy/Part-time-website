<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.clvgy.system.mapper.SysAddressMapper">
    <resultMap id="SysAddressMap" type="com.clvgy.model.system.SysAddress" autoMapping="true">
    </resultMap>

    <!-- 用于select查询公用抽取的列 -->
    <sql id="columns">
      id,enterprise_id,longitude,latitude,create_time,update_time,is_deleted
   </sql>

    <select id="selectPage" resultMap="SysAddressMap">
        select <include refid="columns" />
        from sys_address
        <where>
             is_deleted = 0
        </where>
        order by id desc
    </select>

</mapper>
